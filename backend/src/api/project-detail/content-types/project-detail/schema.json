{
  "kind": "collectionType",
  "collectionName": "project_details",
  "info": {
    "singularName": "project-detail",
    "pluralName": "project-details",
    "displayName": "Project details",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "project_image": {
      "type": "component",
      "repeatable": true,
      "component": "project-image.project-image",
      "required": true
    },
    "company": {
      "type": "string",
      "required": true
    },
    "URL": {
      "type": "uid",
      "targetField": "title"
    },
    "services": {
      "type": "component",
      "repeatable": true,
      "component": "services.services"
    },
    "project": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project",
      "inversedBy": "project_detail"
    }
  }
}
